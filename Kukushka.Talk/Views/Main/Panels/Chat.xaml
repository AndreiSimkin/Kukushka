<UserControl x:Class="Kukushka.Talk.Views.Main.Panles.Chat"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Kukushka.Talk.Views.Main.Panles"
             xmlns:sharedelements="clr-namespace:Kukushka.Talk.Views.Shared.Elements"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800" Background="#242424"
             xmlns:vm="clr-namespace:Kukushka.Talk.ViewModels.Main.Panels"
             >
    <UserControl.DataContext>
        <vm:ChatViewModel/>
    </UserControl.DataContext>
    <Grid>
        <sharedelements:Button Command="{Binding TestClickCommand}" Foreground="AliceBlue" MouseOverBackground="#11FFFFFF" Cursor="Hand">
            Hui
        </sharedelements:Button>
    </Grid>
</UserControl>
